pipeline {
    agent any 
        tools {
            jdk 'openjdk17'
            maven 'Maven-3.8.5'
        }
    stages {
      stage (version) {
        steps {
            sh ''' mvn -version
            java --version '''
        }
      }
      stage (build Version){
        steps {
           sh ''' echo ${Version} '''
           }
      }     
    }
}
